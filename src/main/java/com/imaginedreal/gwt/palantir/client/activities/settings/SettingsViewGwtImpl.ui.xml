<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
    xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget">

    <ui:style>
    </ui:style>

    <mgwt:panel.flex.RootFlexPanel>
        <mgwt:header.HeaderPanel>
            <mgwt:button.image.PreviousitemImageButton ui:field="backButton" />
            <mgwt:panel.flex.FlexSpacer />
            <mgwt:header.HeaderTitle>Settings</mgwt:header.HeaderTitle>
            <mgwt:panel.flex.FlexSpacer />
            <mgwt:panel.flex.FixedSpacer />
        </mgwt:header.HeaderPanel>

        <mgwt:panel.scroll.ScrollPanel scrollingEnabledX="false">
            <g:FlowPanel ui:field="flowPanel">
                <mgwt:list.widgetlist.WidgetList ui:field="widgetList">
                    <mgwt:header>
                        <g:Label text="Font" />
                    </mgwt:header>
                    <mgwt:input.slider.Slider ui:field="slider" max="24" />
                    <g:HTML ui:field="valueField">0</g:HTML>
                </mgwt:list.widgetlist.WidgetList>
            </g:FlowPanel>
        </mgwt:panel.scroll.ScrollPanel>

    </mgwt:panel.flex.RootFlexPanel>
    
</ui:UiBinder>